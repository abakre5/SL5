
#include <stdio.h>
#include <stdlib.h>
#include "mpi.h"

int main( int argc, char **argv ){

	MPI_Status status;
	MPI_Comm client;

	char portName[MPI_MAX_PORT_NAME], ch, str[50];
	int size;

	MPI_Init(&argc, &argv);
	MPI_Comm_size(MPI_COMM_WORLD, &size);
	MPI_Open_port(MPI_INFO_NULL, portName);

	return 0;
}

