#include <stdio.h>
#include <stdlib.h>
#include "mpi.h"

int main(int argc, char **argv) {

	MPI_Status status;
	MPI_Comm server;

	char portName[MPI_MAX_PORT_NAME];

	MPI_Init(&argc, &argv);
	MPI_Comm_connect(&portName,MPI_INFO_NULL, 0, MPI_COMM_WORLD, &server);

	return EXIT_SUCCESS;
}
